{
  "name": "mathjax_playground",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "deploy": "umi gh-pages",
    "test": "umi test",
    "lint:es": "eslint --ext .js src mock tests",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -E GIT_PARAMS"
  },
  "dependencies": {
    "antd": "^4.17.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-mathjax2": "^0.0.2",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "lint-staged": "^12.1.2",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
